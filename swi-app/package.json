{
  "name": "hello-electron",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "run": "electron out/dist",
    "startdevserver": "webpack-dev-server --port=4200",
    "startelectron": "electron src/entry.live.js",
    "start": "concurrently \"webpack-dev-server --port=4200\" \"electron src/entry.live.js\"",
    "patchversion": "cd ./src && npm version patch",
    "build": "webpack",
    "buildrun": "cls && npm run build && npm run run",
    "prepackagebuild": "npm run patchversion",
    "packagebuild": "webpack",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "prepackage": "npm run packagebuild && electron-packager ./out/dist --out=out/bin --platform=win32 --arch=x64 --overwrite",
    "package": "node ./electron-build.js",
    "deploy": "deploy.cmd",
    "packagedeploy": "npm run package && deploy.cmd"
  },
  "private": true,
  "build": {
    "win": {
      "target": "squirrel"
    }
  },
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "electron-gh-releases": "^2.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "font-awesome": "^4.7.0",
    "minimist": "^1.2.0",
    "ng2-toastr": "^4.0.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.1",
    "@ngtools/webpack": "^1.3.0",
    "@types/electron": "^1.4.35",
    "@types/jasmine": "2.5.38",
    "autoprefixer": "^6.5.3",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "electron-winstaller": "^2.5.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-loader": "^0.5.4",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "less-loader": "^2.2.3",
    "npm-build-tools": "^2.2.5",
    "postcss-loader": "^0.13.0",
    "postcss-url": "^5.1.2",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.4.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-server": "~2.3.0"
  }
}
